<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="PROD"
    
    COMPATIBILITY="ALL"
    AUTHOR="Jan Lolling"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    DATA_AUTO_PROPAGATE="false"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>System</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR NAME="STD_OUT" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="ERR_OUT" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"    MAX_OUTPUT="0" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="0" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" />
  </CONNECTORS>

  <PARAMETERS>
  	<PARAMETER NAME="ROOTDIR" FIELD="CHECK" NUM_ROW="1">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
    <PARAMETER NAME="DIRECTORY" FIELD="DIRECTORY" NUM_ROW="1" SHOW_IF="ROOTDIR=='true'">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__"</DEFAULT>
	</PARAMETER>
    
	<PARAMETER NAME="USE_SINGLE_COMMAND" FIELD="RADIO" REQUIRED="true"
		NUM_ROW="10">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>	
	
    <PARAMETER NAME="COMMAND" FIELD="MEMO_JAVA" REQUIRED="true" NUM_ROW="20" SHOW_IF="USE_SINGLE_COMMAND=='true'" >
      <DEFAULT>"cmd /c echo HELLO"</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="USE_ARRAY_COMMAND" FIELD="RADIO" REQUIRED="true"
		NUM_ROW="30" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="ARRAY_COMMAND" FIELD="TABLE" REQUIRED="true"
  				NUM_ROW="40" SHOW_IF="USE_ARRAY_COMMAND=='true'" >
  			<ITEMS>
  				<ITEM NAME="VALUE" />
  			</ITEMS>
 	</PARAMETER>
 	
  	<PARAMETER NAME="MAX_RUNTIME" FIELD="TEXT" NUM_ROW="45">
			<DEFAULT></DEFAULT>
	</PARAMETER>

  	<PARAMETER NAME="OK_EXIT_CODE" FIELD="TEXT" NUM_ROW="47">
			<DEFAULT>0</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="50">
       <ITEMS DEFAULT="NORMAL_OUTPUT">
          <ITEM NAME="NORMAL_OUTPUT" VALUE="NORMAL_OUTPUT" />
          <ITEM NAME="RETRIEVE_OUTPUT" VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>
    
  	<PARAMETER NAME="SEND_ERR_TO_STD_OUT" FIELD="CHECK" NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>

  	<PARAMETER NAME="SEND_TO_CONSOLE" FIELD="CHECK" NUM_ROW="65">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>

  	<PARAMETER NAME="SEND_TO_LOG" FIELD="CHECK" NUM_ROW="67">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="SCHEMA_STDOUT" FIELD="SCHEMA_TYPE" NUM_ROW="70" READONLY="true" CONTEXT="STD_OUT">
  	  <TABLE>
		<COLUMN NAME="outputline" TYPE="id_String" LENGTH="255" CUSTOM="true" READONLY="true"/>
	  </TABLE>
  	</PARAMETER>
  	
    <PARAMETER NAME="SCHEMA_ERROUT" FIELD="SCHEMA_TYPE" NUM_ROW="75" READONLY="true" CONTEXT="ERR_OUT">
  	  <TABLE>
		<COLUMN NAME="outputline" TYPE="id_String" LENGTH="255" CUSTOM="true" READONLY="true"/>
	  </TABLE>
  	</PARAMETER>

  	<PARAMETER NAME="PARAMS" FIELD="TABLE" NUM_ROW="80" NB_LINES="8">
      <ITEMS>
   		<ITEM NAME="NAME"/>
        <ITEM NAME="VALUE"/>
      </ITEMS>
    </PARAMETER>

  	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="90">
			<DEFAULT>true</DEFAULT>
	</PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS> 
  	<PARAMETER NAME="ERROR_DETECTION_STRING" FIELD="TEXT" NUM_ROW="10">
		<DEFAULT>"Exception"</DEFAULT>
	</PARAMETER>
  </ADVANCED_PARAMETERS>
   
  <CODEGENERATION>
  </CODEGENERATION>

  <RETURNS>
    <RETURN AVAILABILITY="AFTER" NAME="STDOUT_NB_LINE" TYPE="id_Integer"/>  
    <RETURN AVAILABILITY="AFTER" NAME="ERROUT_NB_LINE" TYPE="id_Integer"/>  
    <RETURN NAME="OUTPUT" TYPE="id_String" AVAILABILITY="AFTER"/>
    <RETURN NAME="ERROROUTPUT" TYPE="id_String" AVAILABILITY="AFTER"/>
    <RETURN NAME="EXIT_VALUE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="SUCCESSFUL" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="KILLED" TYPE="id_Boolean" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
